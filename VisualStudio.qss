/*=======================================*/
QSpinBox,
QAbstractSpinBox
{
	color: #d8d8d8;
	padding: 2px;
	min-height: 1em;
}

QSpinBox,
QAbstractSpinBox
{
	border: 1px solid #444444;
	background-color: #444444;
}

QTextEdit,
QPlainTextEdit,
QSpinBox,
QAbstractSpinBox
{
	border: 1px solid #2e2e2e;
	background-color: #1f1f1f;
}

QSpinBox,
QAbstractSpinBox
{
	min-width: 2.5em;
	padding-right: 10px;
}

QTextEdit:disabled,
QPlainTextEdit:disabled,
QListWidget:disabled,
QTreeWidget:disabled,
QSpinBox:disabled,
QAbstractSpinBox:disabled
{
	border: 1px solid transparent;
	background-color: #282828;
}

QAbstractSpinBox::drop-down,
QSpinBox::drop-down
{
	background-color: #1d1d1d;
	border: 0;
	
}

QToolButton[popupMode="1"],
QToolButton[popupMode="2"]
{
	padding-right: 14px;
}

QToolButton::menu-arrow,
QToolButton::menu-indicator
{
	image: url(Resources/Themes/MetroDark_res/arrow_down.png);
	subcontrol-origin: padding;
	subcontrol-position: center right;
	top: 2px;
	right: 2px;
	width: 14px;
}

QSpinBox::down-button,
QAbstractSpinBox::down-button
{
	padding-right: 4px;
	image: url(Resources/Themes/MetroDark_res/arrow_down.png);
	width: 14px;
	padding-bottom: 1px;
}

QSpinBox::down-button:disabled,
QAbstractSpinBox::down-button:disabled
{
	image: url(Resources/Themes/MetroDark_res/arrow_down_disabled.png);
}

QSpinBox::up-button,
QAbstractSpinBox::up-button
{
	padding-right: 4px;
	image: url(Resources/Themes/MetroDark_res/arrow_up.png);
	width: 14px;
	padding-top: 1px;
}

QSpinBox::up-button:disabled,
QAbstractSpinBox::up-button:disabled
{
	image: url(Resources/Themes/MetroDark_res/arrow_up_disabled.png);
}

QCheckBox,
QAbstractCheckBox,
QRadioButton
{
	color: #d8d8d8;
	background: transparent;
}

QCheckBox::indicator
{
	background-color: #444444;
	border: 1px solid #444444;
	height: 13px;
	width: 13px;
	margin-top: 1px;
}

QMenu::indicator
{
	width: 12px;
	left: 6px;
}

QCheckBox::indicator:checked,
QMenu::indicator:checked
{
	image: url(Resources/Themes/MetroDark_res/checkbox_check_dark.png);
}

QCheckBox::indicator:disabled
{
	border: 1px solid #2d2d2d;
	background-color: #282828;
}

QCheckBox::indicator:checked:disabled
{
	border: 1px solid transparent;
	image: url(Resources/Themes/MetroDark_res/checkbox_check_disabled.png);
}

QRadioButton::indicator
{
	background: #444444;
	border: 1px solid #444444;
	height: 12px;
	width: 12px;
}

QRadioButton::indicator:disabled
{
	background-color: #282828;
	margin: 1px;
	border: 1px solid transparent;
}

QRadioButton::indicator:checked
{
	image: url(Resources/Themes/MetroDark_res/radio_check_dark.png);
}

QRadioButton::indicator:checked:disabled
{
	image: url(Resources/Themes/MetroDark_res/radio_check_disabled.png);
}

QSlider::groove
{
	background: #393939;
	border: 1px solid #393939;
	font-size: 3pt;
}

QSlider::groove:horizontal
{
	height: 1em;
}

QSlider::groove:vertical
{
	width: 1em;
}

QSlider::groove:disabled,
QSlider::sub-page:disabled
{
	background: #282828;
	border: 1px solid transparent;
}

QSlider::sub-page
{
	background: #486d8d;
	border: 1px solid #486d8d;
}

QSlider::handle
{
	background: #777777;
	border: 1px solid #222222;
	font-size: 4pt;
}

QSlider::handle:horizontal
{
	margin: -5px -1px;
	width: 4.5em;
}

QSlider::handle:vertical
{
	margin: -1px -5px;
	height: 4.5em;
}

QSlider::handle:focus
{
	background-color: #6d96ba;
	border-color: #222266;
}

QSlider::handle:hover
{
	background-color: #999999;
}

QSlider::handle:pressed
{
	background-color: #bbbbbb;
	border-color: #222222;
}

QSlider::handle:disabled
{
	background-color: #282828;
	border: 1px solid #282828;
}

QCheckBox::indicator:focus,
QRadioButton::indicator:focus,
QComboBox:focus
{
	background-color: #1f1f1f;
}

QCheckBox::indicator:focus:hover,
QRadioButton::indicator:focus:hover,
QComboBox:focus:hover
{
	background-color: #1f1f1f;
	border-color: #9e9e9e;
}

QCheckBox::indicator:hover,
QRadioButton::indicator:hover,
QComboBox:hover
{
	background-color: #1f1f1f;
	border-color: #9e9e9e;
}

QSpinBox:focus,
QAbstractSpinBox:focus,
QComboBox:editable:focus
{
	background-color: #1f1f1f;
}

QSpinBox:focus:hover,
QAbstractSpinBox:focus:hover,
QComboBox:editable:focus:hover
{
	border-color: #485d6f;
}

QSpinBox:hover,
QAbstractSpinBox:hover,
QComboBox:editable:hover
{
	background-color: #1f1f1f;
	border-color: #555555;
}

QCheckBox::indicator:focus,
QRadioButton::indicator:focus,
QTextEdit:focus,
QPlainTextEdit:focus,
QSpinBox:focus,
QAbstractSpinBox:focus
{
	border-color: transparent;
	color: #ffffff;
	background-color: #252526;
}

QFontDialog
{
	min-width: 32em;
	min-height: 24em;
}

QColorDialog QColorLuminancePicker
{
	background-color: transparent;
}

QMessageBox,
QDialogButtonBox
{
	dialogbuttonbox-buttons-have-icons: 0;
}
