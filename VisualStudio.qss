	font-size: 13pt;
}
QGroupBox::title 
{
	background-color: transparent;
	margin: 6px;
	margin-left: 8px;
	margin-right: 8px;
}
*/
/*=======================================*/

/*=============== OUTLINE ===============*/
QListView
{
	background-color: #1d1d1d;
}

QListView::item
{
	border: 1px solid transparent;
	color: #d8d8d8;
	selection-color: #d8d8d8;
	margin: 2px;
}

QListView::item:hover 
{
	background-color: #333333;
	border: 1px solid #333333;
}

QListView::item:selected 
{
	background-color: #3b3b3b;
	border: 1px solid #3b3b3b;
}

QListView::item:selected:active
{
	/*background-color: #9e9e9e;*/
	border: 1px solid #9e9e9e;
}

/*=======================================*/

/*=============== LIBRARY ===============*/

QTreeView
{
	background-color: #1d1d1d;
	color: #d8d8d8;
	selection-background-color: #1d1d1d;
	selection-color: #d8d8d8;
	border: 1px solid #1f1f1f;
}

QTreeView::item
{
	min-width: 60px;
	border: 1px solid transparent;
	border-left: 0;
	border-right: 0;
	color: #d8d8d8;
	padding: 2px 4px;
	selection-color: #d8d8d8;
	border-radius: 0;
}

QTreeView::item:first,
QTreeView::item:only-one
{
	border-left: 1px solid transparent;
	border-top-left-radius: 0px;
	border-bottom-left-radius: 0px;
}

QTreeView::item:last,
QTreeView::item:only-one
{
	border-right: 1px solid transparent;
	border-top-right-radius: 0px;
	border-bottom-right-radius: 0px;
}

QTreeView::item:hover,
QTreeView::item:focus
{
	background-color: #1f1f1f;
	border-color: #9e9e9e;
}

QTreeView::item:selected
{
	background-color: #333333;
	border: 0;
	padding-left: 5px;
}

QTreeView::item:selected:active
{
	background-color: #403582;
	border: 1px solid #7160e8;
	padding-left: 4px;
}

QTreeView::branch
{
	border-image: none;
	image: none;
	margin-left: 3px;
	margin-top: 1px;
	padding-left: 3px;
}

QTreeView::branch:has-children:closed
{
	image: url(Resources/Themes/MetroDark_res/branch_closed.png);
}

QTreeView::branch:has-children:open
{
	image: url(Resources/Themes/MetroDark_res/branch_open.png);
}

QTreeView::indicator
{
	color: #d8d8d8;
	background: transparent;
}

QTreeView::indicator
{
	background: #444444;
}

QTreeView::indicator
{
	background-color: #444444;
	border: 1px solid #444444;
	height: 13px;
	width: 13px;
	margin-top: 1px;
}

QTreeView::indicator:checked
{
	image: url(Resources/Themes/MetroDark_res/checkbox_check_dark.png);
}

QTreeView::indicator:disabled
{
	border: 1px solid #2d2d2d;
	background-color: #282828;
}

QTreeView::indicator:checked:disabled
{
	border: 1px solid transparent;
	image: url(Resources/Themes/MetroDark_res/checkbox_check_disabled.png);
}

QTreeView::indicator:focus
{
	background-color: #1f1f1f;
}

QTreeView::indicator:focus:hover
{
	background-color: #1f1f1f;
	border-color: #9e9e9e;
}

QTreeView::indicator:hover
{
	background-color: #1f1f1f;
	border-color: #9e9e9e;
}

QTreeView::indicator:focus
{
	border-color: transparent;
	color: #ffffff;
	background-color: #252526;
}

/*=======================================*/

/*========= Export table thing? =========*/

QHeaderView
{
	border-bottom: 1px solid #1c1c1c;
	border-radius: 0;
}

QHeaderView::section
{
	border: 0;
	background-color: #2d2d2d;
	color: #d8d8d8;
	padding: 4px;
	padding-left: 8px;
	padding-right: 20px;
	border-radius: 0;
}

QHeaderView::down-arrow,
QHeaderView::up-arrow
{
	margin: 1px;
	top: 1px;
	right: 5px;
	width: 14px;
}

QHeaderView::down-arrow
{
	image: url(Resources/Themes/MetroDark_res/arrow_down.png);
}

QHeaderView::up-arrow
{
	image: url(Resources/Themes/MetroDark_res/arrow_up.png);
}

/*=======================================*/

/*=========== Text editor tabs ==========*/

QTabBar
{
    qproperty-drawBase: 0;
	border-bottom: 2px solid #7160e8;
}

QTabBar::tab
{
	margin-left: 2px;
	margin-bottom: 4px;
	color: #ffffff;
	background-color: #434343;
	
	padding: 6px 12px;
}

QTabBar::tab:hover
{
	background-color: #007ACC;
}

QTabBar::tab:disabled
{
	color: #484848;
}

QTabBar::tab:selected
{
	color: #ffffff;
	background-color: #434343;
	margin-bottom: 0px;
}

QTabBar::tab:bottom
{
	border-top: 0;
	margin-top: 1px;
}

QTabBar::tab:top:selected
{
	margin-bottom: 0;
	border-top: 2px solid #7160e8;
}

QTabBar::tab:bottom:selected
{
	margin-top: 0;
	border-left: 2px solid #7160e8;
	border-right: 2px solid #7160e8;
	border-top: 2px solid #7160e8;
}

QTabBar::tab:bottom:only-one,
QTabBar::tab:bottom:first,
QTabBar::tab:top:first,
QTabBar::tab:top:only-one
{
	border-top-left-radius: 0px;
}

QTabBar::tab:bottom:last,
QTabBar::tab:bottom:only-one,
QTabBar::tab:top:last,
QTabBar::tab:top:only-one
{
	border-top-right-radius: 0px;
}
/*=======================================*/

/*============= Scroll bars =============*/
QSizeGrip
{
	  image: url(Resources/Themes/QDark_res/sizegrip.png);
	  width: 12px;
	  height: 12px;
}

QScrollBar:horizontal
{
	  height: 15px;
	  margin: 3px 15px 3px 15px;
	  border: 1px transparent #2A2929;
	  border-radius: 4px;
	  background-color: #2A2929;
}

QScrollBar::handle:horizontal
{
	  background-color: #605F5F;
	  min-width: 5px;
	  border-radius: 4px;
}

QScrollBar::add-line:horizontal
{
	  margin: 0px 3px 0px 3px;
	  border-image: url(Resources/Themes/QDark_res/right_arrow_disabled.png);
	  width: 10px;
	  height: 10px;
	  subcontrol-position: right;
	  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
	  margin: 0px 3px 0px 3px;
	  border-image: url(Resources/Themes/QDark_res/left_arrow_disabled.png);
	  height: 10px;
	  width: 10px;
	  subcontrol-position: left;
	  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover,QScrollBar::add-line:horizontal:on
{
	  border-image: url(Resources/Themes/QDark_res/right_arrow.png);
	  height: 10px;
	  width: 10px;
	  subcontrol-position: right;
	  subcontrol-origin: margin;
}


QScrollBar::sub-line:horizontal:hover, QScrollBar::sub-line:horizontal:on
{
	  border-image: url(Resources/Themes/QDark_res/left_arrow.png);
	  height: 10px;
	  width: 10px;
	  subcontrol-position: left;
	  subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal
{
	  background: none;
}


QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
	  background: none;
}

QScrollBar:vertical
{
	  background-color: #2A2929;
	  width: 15px;
	  margin: 15px 3px 15px 3px;
	  border: 1px transparent #2A2929;
	  border-radius: 4px;
}

QScrollBar::handle:vertical
{
	  background-color: #605F5F;
	  min-height: 5px;
	  border-radius: 4px;
}

QScrollBar::sub-line:vertical
{
	  margin: 3px 0px 3px 0px;
	  border-image: url(Resources/Themes/QDark_res/up_arrow_disabled.png);
	  height: 10px;
	  width: 10px;
	  subcontrol-position: top;
	  subcontrol-origin: margin;
}

QScrollBar::add-line:vertical
{
	  margin: 3px 0px 3px 0px;
	  border-image: url(Resources/Themes/QDark_res/down_arrow_disabled.png);
	  height: 10px;
	  width: 10px;
	  subcontrol-position: bottom;
	  subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover,QScrollBar::sub-line:vertical:on
{

	  border-image: url(Resources/Themes/QDark_res/up_arrow.png);
	  height: 10px;
	  width: 10px;
	  subcontrol-position: top;
	  subcontrol-origin: margin;
}


QScrollBar::add-line:vertical:hover, QScrollBar::add-line:vertical:on
{
	  border-image: url(Resources/Themes/QDark_res/down_arrow.png);
	  height: 10px;
	  width: 10px;
	  subcontrol-position: bottom;
	  subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical
{
	  background: none;
}


QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
	  background: none;
}
