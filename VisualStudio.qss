ApplicationPalette {
  qproperty-window: #2e2e2e;
  qproperty-windowtext: #eeeeee;
  qproperty-windowtext_disabled: #484848;
  qproperty-base: #1d1d1d;
  qproperty-alternatebase: #2d2d2d;
  qproperty-text: #d8d8d8;
  qproperty-text_disabled: #484848;
  qproperty-tooltipbase: #1d1d1d;
  qproperty-tooltiptext: #d8d8d8;
  qproperty-tooltiptext_disabled: #484848;
  qproperty-brighttext: white;
  qproperty-brighttext_disabled: #484848;
  qproperty-highlight: #298ce1;
  qproperty-highlightedtext: white;
  qproperty-highlightedtext_disabled: #484848;
  qproperty-button: #444444;
  qproperty-buttontext: #d8d8d8;
  qproperty-buttontext_disabled: #484848;
  qproperty-link: #39a5dd;
  qproperty-linkvisited: #39a5dd;
  qproperty-light: #1c1c1c;
  qproperty-midlight: transparent;
  qproperty-mid: #1c1c1c;
  qproperty-dark: transparent;
  qproperty-shadow: #1c1c1c;
  qproperty-drawBase: 0;
} 

/*
QLineEdit,QTextEdit,QPlainTextEdit,QAbstractView
{
	border: 4px solid #000000;
}
*/
*,
*::separator,
*::handle,
/**::tab-bar,*/
*::tab,
*::section
{
	font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serif;
	font-size: 10pt;
	margin: 0;
	padding: 0;
	outline: 0;
	border: 0;
	selection-background-color: #298ce1;
	selection-color: white;
	alternate-background-color: transparent;
	color: #eeeeee;
}

/*=========== Main BG color ============*/
QMainWindow
{
	background-color: #1f1f1f;
}
/*=======================================*/

/*============ All windows? =============*/
QFrame 
{
	border-top: 4px solid #434343;
}
/*=======================================*/

/*====== Text labels (preferences) ======*/
QLabel 
{ 
	border-top: unset;
	color: #d8d8d8;
	background: transparent;
}
/*=======================================*/

QDockWidget
{
	/*titlebar-close-icon: url(Resources/Themes/Visual Studio/close.png);*/
	titlebar-normal-icon: url(Resources/Themes/Visual Studio/dock.png);
}

QDockWidget
{
	border: 4px solid #000000;
	titlebar-close-icon: url(Resources/Themes/QDark_res/close.png);
	titlebar-normal-icon: url(Resources/Themes/VisualStudio/dock.png);
	background-color: #1f1f1f;
}

QDialog,
QWizard *,
QCalendarWidget *,
#qswPages > QObject
{
	background-color: #1f1f1f; /* preferences bg */
	color: #d8d8d8;
}

/*============= ALL Widgets =============*/
*:disabled,
*::item:disabled
{
	color: #9e9e9e;
}

*::separator
{
	height: 6px;
	width: 6px;
}

*::separator:hover
{
	background: #333333;
}
/*=======================================*/

/*================= IDK =================*/
a
{
	color: #ff0000;
	text-decoration: none;
}

QToolTip,
QWhatsThis
{
	font-size: 8pt;
	min-height: 1.3em;
	border: 1px solid #888888;
	border-radius: 0;
	background-color: #1d1d1d;
	color: #d8d8d8;
}


/*
QTextBrowser,
QTextEdit
{
	background-color: #1d1d1d;
	color: #d8d8d8;
	border: 1px solid #1c1c1c;
}
*/
/*=======================================*/


/*======= File, edit, tools, ... ========*/
QMenuBar
{
	/*border: unset;*/
}

QMenuBar::item
{
	background-color: transparent;
	padding: 4px 8px;
	margin-top: 4px;
}

QMenuBar::item:selected
{
	background: #3d3d3d;
	border: 1px solid #666666;
	color: white;
}

QMenuBar::item:pressed
{
	background: #1f1f1f;
	color: white;
	border-bottom: unset;
	top:10px;
	margin-top:2px;
}
/*=======================================*/

/*========= Save, save all ... ==========*/
QToolBar
{
	background-color: #1f1f1f;
	spacing: 2px;
	margin: 4px;
}

QToolButton
{
	padding: 2px;
}

QToolButton:on
{
	background-color: #444444;
	border: 1px solid #444444;
}

QToolButton:hover
{
	background-color: #3d3d3d;
	border: 1px solid #9e9e9e;
}

QToolButton:pressed
{
	background-color: #424242;
}

QToolBar::separator
{
	background: #3d3d3d;
	height: 1px;
	margin: 2px;
	width: 2px;
}

QToolBar::separator:hover
{
	background: #555555;
	border: 0;
}

QToolButton#qt_toolbar_ext_button
{
	min-width: 8px;
	width: 8px;
	padding: 1px;
	margin-right: 2px;
	qproperty-icon: url(Resources/Themes/MetroDark_res/toolbar_ext.png);
}

QToolBar::handle:horizontal
{
	image: url(Resources/Themes/MetroDark_res/handle_horizontal.png);
	width: 8px;
	padding: 4px;
}

QToolBar::handle:vertical
{
	image: url(Resources/Themes/MetroDark_res/handle_vertical.png);
	height: 8px;
	padding: 4px;
}
/*=======================================*/

/*========= "All drop downs?" ===========*/
/*======= Except for combo boxes ========*/
QMenu
{
	background: #1f1f1f;
	border: 1px solid #666666;
	color: #000000;
	padding: 3px;
}

QMenu::item 
{
	border: 1px solid transparent;
	color: #e5e5e5;
	padding: 3px 3px 3px 7px;
}

QMenu::item:selected 
{
	background: #333333;
	border: 1px solid #9e9e9e;
}

QMenu::item:disabled
{
	border: 1px solid transparent;
	background: transparent;
	color: #3c3c3c;
}

QMenu::separator
{
	background: #3d3d3d;
	height: 1px;
}

QMenu::indicator
{
	padding-top: 2px;
	height: 25px;
	width: 25px;
}
/*=======================================*/

/*============= All buttons? ============*/

QPushButton
{
	background-color: #3d3d3d;
	border: 0;
	color: white;
	font-size: 10pt;
	padding: 4px 14px;
}

QPushButton:focus
{
	background-color: #403582;
}

QPushButton:hover
{
	background-color: #4d4d4d;
}

QPushButton:pressed,
QPushButton:pressed:focus
{
	background-color: #333333;
}

QPushButton[popupMode="1"],
QPushButton[popupMode="2"]
{
	padding-right: 14px;
}

QPushButton:disabled
{
	border: 1px solid transparent;
	background-color: #282828;
}

QPushButton::menu-arrow,
QPushButton::menu-indicator
{
	image: url(Resources/Themes/MetroDark_res/arrow_down.png);
	subcontrol-origin: padding;
	subcontrol-position: center right;
	top: 2px;
	right: 2px;
	width: 14px;
}
/*=======================================*/

/*======== Export window tabs? ==========*/
QTabWidget::pane
{
	background: #1f1f1f;
	border: 0;
	/*border-top: 2px solid #7160e8;*/ /* Maybe? */
}

